{
  "title": "AssetLocaleComp",
  "$ref": "#/$defs/AssetLocaleComp",
  "$defs": {
    "AssetLocaleComp": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": { "title": "String", "type": "string", "minLength": 1 },
        "isDefault": { "title": "bool", "type": "boolean" },
        "textEntries": {
          "type": "array",
          "items": { "title": "AssetLocaleText", "description": "Translation key / value.", "$ref": "#/$defs/AssetLocaleText" }
        }
      },
      "required": [ "name", "textEntries" ],
      "defaultSnippets": [
        { "label": "New", "body": "^{\n  \"name\": \"placeholder\",\n  \"textEntries\": []\n}" }
      ]
    },
    "AssetLocaleText": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "key": { "title": "StringHash", "type": "string", "minLength": 1 },
        "value": { "title": "String", "type": "string" }
      },
      "required": [ "key", "value" ],
      "defaultSnippets": [
        { "label": "New", "body": "^{\n  \"key\": \"placeholder\",\n  \"value\": \"\"\n}" }
      ]
    }
  }
}