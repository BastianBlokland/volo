{
  "title": "AssetLevel",
  "$ref": "#/$defs/AssetLevel",
  "$defs": {
    "AssetLevel": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": { "title": "String", "type": "string" },
        "terrain": { "title": "AssetRef", "description": "Asset reference", "$ref": "#/$defs/AssetRef" },
        "fogMode": { "title": "AssetLevelFog", "$ref": "#/$defs/AssetLevelFog" },
        "startpoint": { "title": "GeoVector3", "description": "3D Vector", "$ref": "#/$defs/GeoVector3" },
        "objects": {
          "type": "array",
          "items": { "title": "AssetLevelObject", "$ref": "#/$defs/AssetLevelObject" }
        }
      },
      "required": [ "objects" ],
      "defaultSnippets": [
        { "label": "New", "body": "^{\n  \"objects\": []\n}" }
      ]
    },
    "AssetRef": { "title": "StringHash", "type": "string", "minLength": 1 },
    "AssetLevelFog": {
      "enum": [ "Disabled", "VisibilityBased" ]
    },
    "GeoVector3": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "x": { "title": "f32", "type": "number" },
        "y": { "title": "f32", "type": "number" },
        "z": { "title": "f32", "type": "number" }
      },
      "required": [],
      "defaultSnippets": [
        { "label": "New", "body": "^{}" }
      ]
    },
    "AssetLevelObject": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": { "title": "u32", "type": "integer", "exclusiveMinimum": 0, "maximum": 4294967295 },
        "prefab": { "title": "StringHash", "type": "string", "minLength": 1 },
        "faction": { "title": "AssetLevelFaction", "$ref": "#/$defs/AssetLevelFaction" },
        "position": { "title": "GeoVector3", "description": "3D Vector", "$ref": "#/$defs/GeoVector3" },
        "rotation": { "title": "GeoQuat", "description": "Quaternion", "$ref": "#/$defs/GeoQuat" },
        "scale": { "title": "f32", "type": "number" },
        "properties": {
          "type": "array",
          "items": { "title": "AssetProperty", "$ref": "#/$defs/AssetProperty" }
        },
        "sets": {
          "type": "array",
          "maxItems": 8,
          "items": { "title": "StringHash", "type": "string" }
        }
      },
      "required": [ "prefab", "position", "rotation" ],
      "defaultSnippets": [
        { "label": "New", "body": "^{\n  \"prefab\": \"placeholder\",\n  \"position\": {},\n  \"rotation\": {}\n}" }
      ]
    },
    "AssetLevelFaction": {
      "enum": [
        "None",
        "A",
        "B",
        "C",
        "D"
      ]
    },
    "GeoQuat": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "x": { "title": "f32", "type": "number" },
        "y": { "title": "f32", "type": "number" },
        "z": { "title": "f32", "type": "number" },
        "w": { "title": "f32", "type": "number" }
      },
      "required": [],
      "defaultSnippets": [
        { "label": "New", "body": "^{}" }
      ]
    },
    "AssetProperty": {
      "anyOf": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": { "const": "AssetPropertyType_Num" },
            "$name": { "type": "string" },
            "$data": { "title": "f64", "type": "number" }
          },
          "required": [ "$type", "$name", "$data" ]
        },
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": { "const": "AssetPropertyType_Bool" },
            "$name": { "type": "string" },
            "$data": { "title": "bool", "type": "boolean" }
          },
          "required": [ "$type", "$name", "$data" ]
        },
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": { "const": "AssetPropertyType_Vec3" },
            "$name": { "type": "string" },
            "x": { "title": "f32", "type": "number" },
            "y": { "title": "f32", "type": "number" },
            "z": { "title": "f32", "type": "number" }
          },
          "required": [ "$type", "$name" ]
        },
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": { "const": "AssetPropertyType_Quat" },
            "$name": { "type": "string" },
            "x": { "title": "f32", "type": "number" },
            "y": { "title": "f32", "type": "number" },
            "z": { "title": "f32", "type": "number" },
            "w": { "title": "f32", "type": "number" }
          },
          "required": [ "$type", "$name" ]
        },
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": { "const": "AssetPropertyType_Color" },
            "$name": { "type": "string" },
            "r": { "title": "f32", "type": "number" },
            "g": { "title": "f32", "type": "number" },
            "b": { "title": "f32", "type": "number" },
            "a": { "title": "f32", "type": "number" }
          },
          "required": [ "$type", "$name" ]
        },
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": { "const": "AssetPropertyType_Str" },
            "$name": { "type": "string" },
            "$data": { "title": "StringHash", "type": "string" }
          },
          "required": [ "$type", "$name", "$data" ]
        },
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": { "const": "AssetPropertyType_Asset" },
            "$name": { "type": "string" },
            "$data": { "title": "AssetRef", "description": "Asset reference", "$ref": "#/$defs/AssetRef" }
          },
          "required": [ "$type", "$name", "$data" ]
        }
      ],
      "defaultSnippets": [
        { "label": "New AssetPropertyType_Num", "body": "^{\n  \"\\$type\": \"AssetPropertyType_Num\",\n  \"\\$name\": \"MyUnion\",\n  \"\\$data\": 0\n}" },
        { "label": "New AssetPropertyType_Bool", "body": "^{\n  \"\\$type\": \"AssetPropertyType_Bool\",\n  \"\\$name\": \"MyUnion\",\n  \"\\$data\": false\n}" },
        { "label": "New AssetPropertyType_Vec3", "body": "^{\n  \"\\$type\": \"AssetPropertyType_Vec3\",\n  \"\\$name\": \"MyUnion\"\n}" },
        { "label": "New AssetPropertyType_Quat", "body": "^{\n  \"\\$type\": \"AssetPropertyType_Quat\",\n  \"\\$name\": \"MyUnion\"\n}" },
        { "label": "New AssetPropertyType_Color", "body": "^{\n  \"\\$type\": \"AssetPropertyType_Color\",\n  \"\\$name\": \"MyUnion\"\n}" },
        { "label": "New AssetPropertyType_Str", "body": "^{\n  \"\\$type\": \"AssetPropertyType_Str\",\n  \"\\$name\": \"MyUnion\",\n  \"\\$data\": \"\"\n}" },
        { "label": "New AssetPropertyType_Asset", "body": "^{\n  \"\\$type\": \"AssetPropertyType_Asset\",\n  \"\\$name\": \"MyUnion\",\n  \"\\$data\": \"placeholder\"\n}" }
      ]
    }
  }
}