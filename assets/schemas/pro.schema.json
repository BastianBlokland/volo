{
  "title": "AssetProductMapDef",
  "$ref": "#/$defs/AssetProductMapDef",
  "$defs": {
    "AssetProductMapDef": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "sets": {
          "type": "array",
          "items": {
            "title": "AssetProductSetDef",
            "$ref": "#/$defs/AssetProductSetDef"
          }
        }
      },
      "required": [
        "sets"
      ],
      "defaultSnippets": [
        {
          "label": "New",
          "body": "^{\n  \"sets\": []\n}"
        }
      ]
    },
    "AssetProductSetDef": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "title": "String",
          "type": "string",
          "minLength": 1
        },
        "products": {
          "type": "array",
          "minItems": 1,
          "items": {
            "title": "AssetProductDef",
            "$ref": "#/$defs/AssetProductDef"
          }
        }
      },
      "required": [
        "name",
        "products"
      ],
      "defaultSnippets": [
        {
          "label": "New",
          "body": "^{\n  \"name\": \"placeholder\",\n  \"products\": [\n    {\n      \"\\$type\": \"AssetProduct_Unit\",\n      \"unitPrefab\": \"placeholder\"\n    }\n  ]\n}"
        }
      ]
    },
    "AssetProductDef": {
      "anyOf": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": {
              "const": "AssetProduct_Unit"
            },
            "$name": {
              "type": "string"
            },
            "icon": {
              "title": "String",
              "type": "string"
            },
            "costTime": {
              "title": "f32",
              "type": "number"
            },
            "queueMax": {
              "title": "u16",
              "type": "integer",
              "minimum": 0,
              "maximum": 65535
            },
            "queueBulkSize": {
              "title": "u16",
              "type": "integer",
              "minimum": 0,
              "maximum": 65535
            },
            "unitPrefab": {
              "title": "String",
              "type": "string",
              "minLength": 1
            }
          },
          "required": [
            "$type",
            "unitPrefab"
          ]
        }
      ],
      "defaultSnippets": [
        {
          "label": "New AssetProduct_Unit",
          "body": "^{\n  \"\\$type\": \"AssetProduct_Unit\",\n  \"unitPrefab\": \"placeholder\"\n}"
        }
      ]
    }
  }
}