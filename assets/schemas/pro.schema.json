{
  "title": "AssetProductMapDef",
  "$ref": "#/$defs/AssetProductMapDef",
  "$defs": {
    "AssetProductMapDef": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "sets": {
          "type": "array",
          "items": {
            "title": "AssetProductSetDef",
            "$ref": "#/$defs/AssetProductSetDef"
          }
        }
      },
      "required": [
        "sets"
      ],
      "defaultSnippets": [
        {
          "label": "New",
          "body": "^{\n  \"sets\": []\n}"
        }
      ]
    },
    "AssetProductSetDef": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "title": "String",
          "type": "string",
          "minLength": 1
        },
        "products": {
          "type": "array",
          "minItems": 1,
          "items": {
            "title": "AssetProductDef",
            "$ref": "#/$defs/AssetProductDef"
          }
        }
      },
      "required": [
        "name",
        "products"
      ],
      "defaultSnippets": [
        {
          "label": "New",
          "body": "^{\n  \"name\": \"placeholder\",\n  \"products\": [\n    {\n      \"\\$type\": \"AssetProduct_Unit\",\n      \"unitPrefab\": \"placeholder\"\n    }\n  ]\n}"
        }
      ]
    },
    "AssetProductDef": {
      "anyOf": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": {
              "const": "AssetProduct_Unit"
            },
            "$name": {
              "type": "string"
            },
            "meta": {
              "title": "AssetProductMetaDef",
              "$ref": "#/$defs/AssetProductMetaDef"
            },
            "unitPrefab": {
              "title": "String",
              "type": "string",
              "minLength": 1
            },
            "unitCount": {
              "title": "u32",
              "type": "integer",
              "exclusiveMinimum": 0,
              "maximum": 4294967295
            }
          },
          "required": [
            "$type",
            "unitPrefab"
          ]
        },
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "$type": {
              "const": "AssetProduct_Placable"
            },
            "$name": {
              "type": "string"
            },
            "meta": {
              "title": "AssetProductMetaDef",
              "$ref": "#/$defs/AssetProductMetaDef"
            },
            "prefab": {
              "title": "String",
              "type": "string",
              "minLength": 1
            }
          },
          "required": [
            "$type",
            "prefab"
          ]
        }
      ],
      "defaultSnippets": [
        {
          "label": "New AssetProduct_Unit",
          "body": "^{\n  \"\\$type\": \"AssetProduct_Unit\",\n  \"unitPrefab\": \"placeholder\"\n}"
        },
        {
          "label": "New AssetProduct_Placable",
          "body": "^{\n  \"\\$type\": \"AssetProduct_Placable\",\n  \"prefab\": \"placeholder\"\n}"
        }
      ]
    },
    "AssetProductMetaDef": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "title": "String",
          "type": "string"
        },
        "icon": {
          "title": "String",
          "type": "string"
        },
        "costTime": {
          "title": "f32",
          "type": "number"
        },
        "queueMax": {
          "title": "u16",
          "type": "integer",
          "minimum": 0,
          "maximum": 65535
        },
        "queueBulkSize": {
          "title": "u16",
          "type": "integer",
          "minimum": 0,
          "maximum": 65535
        },
        "cooldown": {
          "title": "f32",
          "type": "number"
        },
        "soundReadyId": {
          "title": "String",
          "type": "string",
          "minLength": 1
        },
        "soundReadyGain": {
          "title": "f32",
          "type": "number"
        },
        "soundCancelId": {
          "title": "String",
          "type": "string",
          "minLength": 1
        },
        "soundCancelGain": {
          "title": "f32",
          "type": "number"
        },
        "soundSuccessId": {
          "title": "String",
          "type": "string",
          "minLength": 1
        },
        "soundSuccessGain": {
          "title": "f32",
          "type": "number"
        }
      },
      "required": [],
      "defaultSnippets": [
        {
          "label": "New",
          "body": "^{}"
        }
      ]
    }
  }
}