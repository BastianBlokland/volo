{
  "name": "volo",
  "publisher": "bastianblokland",
  "displayName": "volo",
  "description": "Ide support for the Volo engine",
  "license": "MIT",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/bastianblokland/volo"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension",
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.75.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "contributes": {
    "languages": [
      {
        "id": "volo-script",
        "aliases": [ "Volo Script" ],
        "extensions": [ ".script" ],
        "configuration": "./script-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "volo-script",
        "scopeName": "script",
        "path": "./syntaxes/script.tmLanguage"
      }
    ],
    "problemMatchers": [
      {
        "name": "volo-repl",
        "owner": "volo",
        "fileLocation": "absolute",
        "pattern": [
          {
            "regexp": "^(.*):(\\d+):(\\d+)-(\\d+):(\\d+):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "endLine": 4,
            "endColumn": 5,
            "message": 6
          }
        ]
      }
    ]
  }
}
